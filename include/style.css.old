/* Header */
header {
	display:			flex;
	justify-content:	center;
	align-items:		center;
	font-family:		'Supercell';
}
/* Fonts */
@font-face {
    font-family:		'Supercell';
    src:				url('Supercell-magic-webfont.ttf') format('truetype');
    font-weight:		normal;
    font-style:			normal;
}
/* Menu */
nav ul{
	list-style-type: 	none;
	display:			flex;
    justify-content:	center/* space-around */;
    align-items:		center;
}
nav li {
	margin-left:		20px;
	margin-right:		20px;
	padding:			20px;
	background-image:	url('../images/wooden-sign.png');
	background-size:	100% 100%;
}
nav a {
	text-decoration: 	none;
	color:				white;
	font-family:		'Supercell';
	font-size:			10px;
	text-shadow:		rgba(0, 0, 0, 0.5) -2px 3px 0px;
}
/* Section */
section {
	margin-top:			50px;
	list-style-type: 	none;
	display:			flex;
    justify-content:	center;
    align-items:		center;
}
section h2 {
	font-family:		'Supercell';
}
section table {
	border-style:		solid;
    border-width:		5px;
	border-collapse:	collapse;
}
section th {
	
}
section td {
	border-style:		solid;
	margin-top:			10cm;
}
section td#PlayerName {
	font-family:		'Supercell';
}

/* Player profile */
div#level {
	width:						50px;
	height:						50px;
	background-image:			url('../images/level.png');
	background-position:		center;
	background-repeat:			no-repeat;
	background-size:			50px 48px;
	padding-top:				30px;
	font-family:				'Supercell';
	font-size:					11px;
	color:						rgb(255, 255, 255);
	text-align:					center;
	text-shadow: 				rgba(0, 0, 0, 0.1) -2px 3px 0px;
}
div#playername {
	font-family:				'Supercell';
	font-size:					18px;
	margin-left:				50px;
	margin-right:				50px;
}
div#items {
	display: 					flex;
	flex-wrap: 					wrap;
	width: 						800px;
	justify-content: 			space-around;
}
div#items div p {
	border-bottom-left-radius:	5px;
	border-bottom-right-radius:	5px;
	border-top-left-radius:		5px;
	border-top-right-radius:	5px;
	color:						rgb(255, 255, 255);
	font-family:				'Supercell';
	margin:						0px;
	padding-top:				5px;
	text-align:					center;
	position:					relative;
}
.Troop {
	width:						140px;
}
.Troop img {
	height:						70px;
	width:						140px;
	border-bottom-left-radius:	5px;
	border-bottom-right-radius:	5px;
	border-top-left-radius:		5px;
	border-top-right-radius:	5px;
}
.Troop p, .Spell p {
	font-size:					11px;
	height:						20px;
	width:						25px;
	left:						100px;
	bottom:						50px;
}
.Spell {
	width:						140px;
}
.Spell img {
	height:						70px;
	width:						140px;
	border-bottom-left-radius:	5px;
	border-bottom-right-radius:	5px;
	border-top-left-radius:		5px;
	border-top-right-radius:	5px;
}
.Heroe {
	width:						200px;
}
.Heroe img {
	height:						100px;
	width:						200px;
	border-bottom-left-radius:	5px;
	border-bottom-right-radius:	5px;
	border-top-left-radius:		5px;
	border-top-right-radius:	5px;
}
.Heroe p {
	font-size:					14px;
	height:						26px;
	width:						35px;
	left:						150px;
	bottom:						70px;
}
div [data-item='normal'] p {
	background-color:			rgb(68, 68, 68);
}
div [data-item='max'] p {
	background-color:			rgb(255, 220, 68);
}

/* Daily */
.memberList {
	background-color:			rgb(214, 204, 186);
	display: 					flex;
	line-height:				36px;
	text-align:					center;
	vertical-align:				top;
	padding-left:				10px;
	padding-right:				10px;
	padding-top:				10px;
	margin-top:					10px;
	border-bottom-left-radius:	10px;
	border-bottom-right-radius:	10px;
	border-top-left-radius:		10px;
	border-top-right-radius:	10px;
}
.position {
	background-color:			rgb(68, 68, 68);
	border-bottom-left-radius:	10px;
	border-bottom-right-radius:	10px;
	border-top-left-radius:		10px;
	border-top-right-radius:	10px;
	font-family:				'Supercell';
	font-size:					14px;
	color:						rgb(255, 255, 255);
	height: 					36px;
	width:						36px;
	text-rendering: 			optimizelegibility;
	text-shadow: 				rgba(0, 0, 0, 0.1) 0px 3px 0px;
}
.evolution {
	width:						40px;
	margin-left:				5px;
}
div [data-position='up'] {
	background-image:			url('../images/up.png');
	background-position:		center;
	background-repeat:			no-repeat;
	background-size: 			36px 36px;
	margin-bottom: 				10px;
}
div [data-position='down'] {
	background-image:			url('../images/down.png');
	background-position:		center;
	background-repeat:			no-repeat;
	background-size: 			36px 36px;
	margin-bottom: 				10px;
}
div [data-position='equal'] {
	background-image:			url('../images/equal.png');
	background-position:		center;
	background-repeat:			no-repeat;
	background-size: 			36px 36px;
	margin-bottom: 				10px;
}
div [data-position='new'] {
	background-image:			url('../images/new.png');
	background-position:		center;
	background-repeat:			no-repeat;
	background-size: 			36px 36px;
	margin-bottom: 				10px;
}
.league {
	width:						50px;
}
.level {
	width:						50px;
	background-image:			url('../images/level.png');
	background-position:		center;
	background-repeat:			no-repeat;
	background-size:			45px 42px;
	margin-bottom:				10px;
	margin-right:				10px;
	font-family:				'Supercell';
	font-size:					11px;
	color:						rgb(255, 255, 255);
	text-shadow: 				rgba(0, 0, 0, 0.1) -2px 3px 0px;
}
.playerName {
	display:					inline-block;
	flex-grow:					1;
	text-align:					left;
}
.playerName p {
	display:					inline-block;
	font-family:				'Supercell';
	margin:						0px;
	vertical-align:				top;
}
.playerName img {
	display:					inline-block;
	width:						36px;
	height:						36px;
	margin-left:				4px;
}
.trophies {
	display:					inline-block;
	background-color:			rgb(226, 196, 90);
	border-bottom-left-radius:	10px;
	border-bottom-right-radius:	10px;
	border-top-left-radius:		10px;
	border-top-right-radius:	10px;
	height: 					36px;
	width:						100px;
	text-shadow: 				rgba(0, 0, 0, 0.1) 0px 3px 0px;
}
.trophies p {
	display:					inline-block;
	font-family:				'Supercell';
	font-size:					14px;
	color:						rgb(255, 255, 255);
	margin:						0px;
	vertical-align:				top;
}
.trophies img {
	display:					inline-block;
	margin-top:					3px;
	margin-left:				3px;
	width:						30px;
	height:						30px;
}
.troops {
	width:						80px;
}
.ratio {
	width:						60px;
}
div[data-ratio='bad'] {
	color:						rgb(255, 0, 0);
}
div[data-ratio='good'] {
	color:						rgb(0, 157, 0);
}
#lastupdate {
	width: 910px;
}